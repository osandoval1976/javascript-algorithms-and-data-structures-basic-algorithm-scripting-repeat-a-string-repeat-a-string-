function repeatStringNumTimes(str, num) {
  // repeat after me
let empty="";
let text=[];
if(num > 0){
  for(let i=0; i < num;  i++){
    text+=str;
  }
return text;
}else{
console.log(text);
return empty;
} 
}
repeatStringNumTimes("abc", -2);

